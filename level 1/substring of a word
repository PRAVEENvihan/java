import java.io.*;
import java.util.*;

class GFG {
	public static void main (String[] args) {
		Scanner s = new Scanner(System.in);
	
       String input = s.nextLine();
      for(int i = 0 ; i<input.length(); i++){
	         for(int j =i ; j<=input.length();j++){
	             System.out.print(input.substring(i,j));
	              System.out.println();
	         }
	         System.out.println();
	     }
	}
	 
}
